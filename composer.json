{
    "name": "backappspace/laravel-conekta-cashier",
    "description": "laravel-conekta-cashier nos da una interface para cobrar subscripciones con Conketa en Laravel. (fork of Backappspace Cashier)",
    "keywords": ["backappspace", "conekta", "laravel", "billing"],
    "license": "MIT",
    "authors": [
        {
            "name": "backappspace",
            "email": "opensource@backapp.software",
            "homepage": "https://github.com/backappspace/laravel-conekta-cashier"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "conekta/conekta-php": "4.0.4",
        "illuminate/filesystem": "5.7.*|5.8.*",
        "illuminate/support": "5.7.*|5.8.*",
        "nesbot/carbon": "^1.26.3 || ^2.0",
        "symfony/http-kernel": "^4.1"
    },
    "require-dev": {
        "illuminate/routing": "^4.1",
        "illuminate/view": "5.7.*|5.8.*",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-0": {
            "Backappspace\\ConektaCashier\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "dev"
}
